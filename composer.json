{
    "name": "kapxapot/associations",
    "description": "Game of associations",
    "keywords": [ "Plasticode", "game", "associations" ],
    "homepage": "https://github.com/kapxapot/associations",
    "license": "MIT",
    "authors": [
        {
            "name": "Sergey Atroshchenko (kapxapot)",
            "email": "kapxapot@gmail.com",
            "homepage": "http://kapxapot.ru"
        }
    ],
    "require": {
        "kapxapot/plasticode": "dev-master",
        "robmorgan/phinx": "^0.10.7",
        "guzzlehttp/guzzle": "^7.2"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/kapxapot/plasticode"
        }
    ],
    "require-dev": {
        "phpunit/phpunit": "^9",
        "phpspec/prophecy": "^1.10.3",
        "phpspec/prophecy-phpunit": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src",
            "Brightwood\\": "brightwood"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests",
            "Brightwood\\Tests\\": "brightwood_tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    }
}
